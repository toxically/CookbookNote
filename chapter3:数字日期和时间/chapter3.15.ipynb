   "source": [
    "# datime.strptime()性能低下\n",
    "# 如果知道确切的格式\n",
    "# 可以试着：\n",
    "from datetime import datetime\n",
    "\n",
    "def parse_ymd(s):\n",
    "    year_s, mon_s, day_s = s.split('-')\n",
    "    return datetime(int(year_s), int(mon_s), int(day_s))\n"
   ]
  }
